<Window x:Class="MvvmCleanCode.BackUpView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MvvmCleanCode" x:Name="backup"
        mc:Ignorable="d" 
        Title="BackUpView" >
    <Grid>
        <Button Command="{Binding BackUp}" CommandParameter="{Binding ElementName=backup}"
                Content="BackUp" 
                HorizontalAlignment="Left" 
                Margin="363,191,0,0" 
                VerticalAlignment="Top" 
                Width="75"/>
        <Label Content="{Binding Result}" HorizontalAlignment="Stretch" Height="120" VerticalAlignment="Top"/>

    </Grid>
</Window>
